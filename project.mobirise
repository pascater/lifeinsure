          "_name": "pro.custom-html",
          "_sourceTheme": "inspirem5",
          "_anchor": "custom-html-31",
          "_PHPplaceholders": [],
          "_JSplaceholders": [
            "<script>\n$(document).ready(function() {\n  $(\"#preventivo-form\").submit(function(event) {\n    event.preventDefault();\n    $(\"#preventivo-risultato\").hide(); \n\n    var formData = {\n      origin: 20220101,\n      birthdate: $(\"#birthdate\").val().replace(/-/g, ''), // Rimossa conversione a numero\n      smoker: $(\"#smoker\").val() === \"Sì\",\n      duration: $(\"#duration\").val() ? parseInt($(\"#duration\").val()) : null,\n      coverage: $(\"#coverage\").val() ? parseInt($(\"#coverage\").val()) : null\n    };\n\n    console.log(\"JSON inviato all'API:\", JSON.stringify(formData)); // LOG IMPORTANTE\n\n    var apiKey = \"yra9qc9WffYaEsKlib8IrQwc2GRLdUCEJIzOo4uTfyc\";\n\n    fetch('https://squarelifetest.asp.lifeware.ch/squarelife_protection/api/v0/switzerland/life_insurance/premium', {\n      method: 'POST',\n      headers: {\n        'Content-Type': 'application/json',\n        'X-SQUARELIFE-APIKEY': apiKey\n      },\n      body: JSON.stringify(formData)\n    })\n    .then(response => {\n      console.log(\"Risposta API:\", response);\n      if (!response.ok) {\n        throw new Error(\"Errore HTTP: \" + response.status);\n      }\n      return response.json();\n    })\n    .then(data => {\n      console.log(\"Dati API ricevuti:\", data);\n      if (data.status === \"ok\") {\n        var premio = data.premium[0].yearly_gross_premium;\n        $(\"#preventivo-risultato\").html(`💵 Il premio annuale è: <strong>${premio} ${data.currency}</strong>`);\n      } else {\n        $(\"#preventivo-risultato\").html(\"⚠️ Errore nel calcolo del preventivo.\");\n      }\n      $(\"#preventivo-risultato\").fadeIn();\n    })\n    .catch(error => {\n      $(\"#preventivo-risultato\").html(\"❌ Errore di comunicazione: \" + error.message);\n      $(\"#preventivo-risultato\").fadeIn();\n      console.error(\"Errore di comunicazione:\", error);\n    });\n  });\n});\n\n</script>"
          ]
