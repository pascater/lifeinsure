          "_JSplaceholders": [
            "<script>\n        function setCookiePreferences(accepted) {\n            localStorage.setItem(\"cookieAccepted\", accepted);\n            document.getElementById(\"cookieBanner\").style.display = \"none\";\n            document.getElementById(\"cookieIcon\").style.display = \"flex\";\n        }\n        \n        function showSettings() {\n            document.getElementById(\"cookieSettings\").style.display = \"block\";\n        }\n        \n        function savePreferences() {\n            let performance = document.getElementById(\"performanceCookies\").checked;\n            let marketing = document.getElementById(\"marketingCookies\").checked;\n            localStorage.setItem(\"performanceCookies\", performance);\n            localStorage.setItem(\"marketingCookies\", marketing);\n            document.getElementById(\"cookieSettings\").style.display = \"none\";\n        }\n        \n        window.onload = function() {\n            if (localStorage.getItem(\"cookieAccepted\") !== null) {\n                document.getElementById(\"cookieBanner\").style.display = \"none\";\n                document.getElementById(\"cookieIcon\").style.display = \"flex\";\n            } else {\n                document.getElementById(\"cookieIcon\").style.display = \"none\";\n            }\n        }\n    </script>"
          ]
        }
